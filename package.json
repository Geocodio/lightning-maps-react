{
  "name": "lightning-maps-react",
  "version": "0.0.23",
  "main": "dist/build.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf dist",
    "clean:dev": "rimraf examples/dist",
    "dev": "npm run clean:dev && webpack-dev-server --config examples/webpack.config.js --mode development",
    "test": "npm run standard",
    "standard": "standard \"**/*.js\"",
    "bump": "npx standard-version"
  },
  "dependencies": {
    "lightning-maps": "^0.0.19",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-react": "^6.24.1",
    "eslint-config-standard-react": "^7.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "husky": "^1.3.1",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "standard-version": "^4.4.0",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "resolutions": {
    "terser": "3.14.1"
  }
}
